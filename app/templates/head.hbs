{{#if model.title}}
  <title>{{model.title}}</title>
{{/if}}

{{#if model.fullName}}
  <meta property="og:site_name" content={{model.fullName}} />
{{/if}}

{{#if model.title}}
  <meta property="og:title" content={{model.title}} />
{{/if}}

{{#if model.type}}
  <meta property="og:type" content={{model.type}} />
{{/if}}

{{#if model.description}}
  <meta name="description" content={{model.description}} />
  <meta property="og:description" content={{model.description}} />
{{/if}}

{{#if model.imageHeight}}
  <meta property="og:image:height" content={{model.imageHeight}} />
{{/if}}

{{#if model.imageUrl}}
  <meta property="og:image" content={{model.imageUrl}} />
{{/if}}

{{#if model.imageWidth}}
  <meta property="og:image:width" content={{model.imageWidth}} />
{{/if}}

{{#if model.canonicalUrl}}
  <meta property="og:url" content={{model.canonicalUrl}} />
{{/if}}

{{#if model.locale}}
  <meta property="og:locale" content={{model.locale}} />
{{/if}}

{{#if model.articleAuthor}}
  <meta name="author" content={{model.articleAuthor}} />
  <meta property="og:article:author" content={{model.articleAuthor}} />
{{/if}}

{{#if model.articleModifiedTime}}
  <meta property="og:article:modified_time" content={{to-iso-string model.articleModifiedTime}} />
{{/if}}

{{#if model.articlePublishedTime}}
  <meta property="og:article:published_time" content={{to-iso-string model.articlePublishedTime}} />
{{/if}}

{{#if model.canonicalUrl}}
  <link rel="canonical" href={{model.canonicalUrl}} />
{{/if}}

{{#if model.faviconUrl}}
  <link rel="icon" href={{model.faviconUrl}}>
{{/if}}

{{#if model.firstName}}
  <meta property="og:profile:first_name" content={{model.firstName}} />
{{/if}}

{{#if model.gender}}
  <meta property="og:profile:gender" content={{model.gender}} />
{{/if}}

{{#if model.lastName}}
  <meta property="og:profile:last_name" content={{model.lastName}} />
{{/if}}
